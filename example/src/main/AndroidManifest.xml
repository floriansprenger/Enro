<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nav.enro.example">

    <application
        android:name="nav.enro.example.ExampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="nav.enro.example.feature.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="nav.enro.example.feature.DashboardActivity" />
        <activity android:name="nav.enro.example.feature.DetailActivity" />
        <activity android:name="nav.enro.example.feature.ListActivity" />
        <activity android:name="nav.enro.example.feature.SearchActivity" />
        <activity android:name="nav.enro.example.feature.UserActivity" />

    </application>

</manifest>